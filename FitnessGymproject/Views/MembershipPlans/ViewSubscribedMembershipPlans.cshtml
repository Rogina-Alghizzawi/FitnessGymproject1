@{
    ViewData["Title"] = "Subscribed Membership Plans";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Plan Name</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Invoice Id</th>
            <th>Total Amount</th>
            <th>Payment Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@(item.PlanName ?? "N/A")</td>
                <td>@(item.StartDate?.ToString("d") ?? "N/A")</td>
                <td>@(item.EndDate?.ToString("d") ?? "N/A")</td>
                <td>@(item.InvoiceId ?? 0)</td>
                <td>@(item.TotalAmount?.ToString("C") ?? "N/A")</td>
                <td>@(item.PaymentStatus ?? "N/A")</td>
            </tr>
        }
    </tbody>
</table>
