@model IEnumerable<FitnessGymproject.Models.Workoutplan>

@{
    ViewData["Title"] = "Subscribed Workout Plans";
}

<h2>Subscribed Workout Plans</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Plan Name</th>
            <th>Exercises</th>
            <th>Schedule</th>
            <th>Goals</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var plan in Model)
        {
            <tr>
                <td>@plan.PlanName</td>
                <td>@plan.Exercises</td>
                <td>@plan.Schedule</td>
                <td>@plan.Goals</td>
                <td>@plan.Price</td>
            </tr>
        }
    </tbody>
</table>
